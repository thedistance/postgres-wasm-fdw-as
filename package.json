{
  "name": "postgres-wasm-fdw-as",
  "version": "0.1.0",
  "description": "Postgres Wasm FDW implemented in AssemblyScript",
  "main": "index.cjs",
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug --config asconfig.json",
    "asbuild:release": "asc assembly/index.ts --target release --config asconfig.json",
    "asbuild": "asc assembly/component-model.ts --target release --config asconfig.json",
    "asbuild:minimal:debug": "asc assembly/minimal-static.ts --target debug --config asconfig.json",
    "asbuild:minimal:release": "asc assembly/minimal-static.ts --target release --config asconfig.json",
    "asbuild:minimal": "npm run asbuild:minimal:debug && npm run asbuild:minimal:release",
    "asbuild:component": "asc assembly/component-model.ts --target release --config asconfig.json --outFile build/component-model.wasm --textFile build/component-model.wat --exportRuntime",
    "build": "npm run asbuild:minimal && node index.cjs > static-data-output.csv",
    "build:component": "npm run asbuild:component && npm run test:component && node scripts/convert-to-component.js",
    "prebuild": "npm install",
    "postbuild": "echo 'Static data exported to static-data-output.csv'",
    "test": "node tests/index.js",
    "test:supabase": "node tests/supabase-simulator.js",
    "test:csv": "node tests/basic-csv-test.js",
    "test:http": "node tests/http-test.js",
    "test:exports": "node tests/export-functions.js",
    "test:mock": "node tests/mock-response-test.js",
    "test:minimal": "npm run asbuild:minimal && node tests/minimal-static-test.js",
    "test:simple": "npm run asbuild:minimal && node tests/simple-minimal-test.js",
    "test:hardcoded": "node tests/hardcoded-data-test.js",
    "test:rows": "npm run asbuild:minimal && node tests/wasm-rows-test.js",
    "test:static": "node tests/static-data-test.js",
    "test:component": "npm run asbuild:component && node tests/component-model-test.js",
    "verify:component": "npm run build:component && node tests/verify-component-compatibility.js",
    "start": "npx serve .",
    "compare": "node tests/compare-implementations.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@bytecodealliance/jco": "^1.10.2",
    "assemblyscript": "^0.27.22",
    "assemblyscript-json": "^1.1.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts",
      "require": "./index.cjs"
    }
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.27.34",
    "as-wasi": "^0.6.0",
    "csv-parse": "^5.6.0",
    "node-fetch": "^3.3.2"
  }
}